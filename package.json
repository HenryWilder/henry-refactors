{
  "name": "henryrefactors",
  "displayName": "Henry Refactors",
  "description": "Custom refactors created by Henry",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "web-palette",
          "title": "Web Palette",
          "icon": "resources/web-palette.svg"
        }
      ]
    },
    "views": {
      "web-palette": [
        {
          "type": "webview",
          "id": "named-colors",
          "name": "Named Colors"
        },
        {
          "type": "webview",
          "id": "common",
          "name": "Common colors"
        },
        {
          "type": "webview",
          "id": "favorites",
          "name": "Favorites"
        }
      ]
    },
    "commands": [
      {
        "command": "henryrefactors.henryAlign",
        "title": "Henry Align",
        "category": "Henry Refactors"
      },
      {
        "command": "henryrefactors.cssPreview",
        "title": "CSS Preview",
        "category": "Henry Refactors"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "emmet": "^2.4.4"
  }
}
